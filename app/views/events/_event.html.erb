<div id="<%= dom_id event %>" class="event" >
  <p>
    <strong>Event name:</strong>
    <%= event.event_name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Event date:</strong>
    <%= event.event_date %>
  </p>

  <p style="display: inline;">
    <strong>Price:</strong>
  <div id="price" style="display: inline;"><%= event.price %></div>
  </p>

  <p>
    <strong>Event Admin User Id:</strong>
    <%= event.admin_user_id %>
  </p>
  
</div>
